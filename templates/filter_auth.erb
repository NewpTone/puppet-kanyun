[filter:auth]
paste.filter_factory = keystone.middleware.auth_token:filter_factory
auth_host = <%=auth_host%>
auth_port = 35357
auth_protocol = http
admin_token = <%=admin_token%>

[composite:kanyun_composite]
use = call:kanyun.server.rest_api_service:kanyun_pipline_factory
keystone = main

[app:main]
paste.app_factory = kanyun.server.rest_api_service:kanyun_service_app_factory

